<?xml version="1.0" encoding="UTF-8"?> 
<beans xmlns="http://www.springframework.org/schema/beans" 
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
xsi:schemaLocation="http://www.springframework.org/schema/beans 
http://www.springframework.org/schema/beans/spring-beans.xsd"> 
<bean id="product" class="com.spring.first.ShoppingProject.ProductClass">
	  <constructor-arg value="1212"/>
	  <constructor-arg value="Laptop"/>
	  <constructor-arg value="55000"/>
	  <constructor-arg value="25"/>
	 </bean>
	 <bean id="order" class="com.spring.first.ShoppingProject.OrderClass">
		<constructor-arg value="1"/>
		<constructor-arg ref="product"/>
	 </bean>
</beans>